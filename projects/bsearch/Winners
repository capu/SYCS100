# @02594803
def bsearch(N,target):
    Low=0
    High=len(N)-1        
    while(Low<=High):
        mid=(Low+High)/2        
        if(N[mid]==target):
                return mid
        elif(N[mid]<target):
            Low=mid+1            
        elif(N[mid]>target):
                 High=mid-1        
    return -1

